{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "singularName": "product-category",
    "pluralName": "product-categories",
    "displayName": "Product-Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "sub_product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-product-category.sub-product-category",
      "inversedBy": "product_categories"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "product_categories"
    }
  }
}
