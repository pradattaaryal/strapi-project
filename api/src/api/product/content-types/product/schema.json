{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "img2": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "isNew": {
      "type": "boolean",
      "default": false
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "sub_product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-product-category.sub-product-category",
      "inversedBy": "products"
    },
    "product_varients": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-varient.product-varient",
      "mappedBy": "product"
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "products"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "featured",
        "normal",
        "trending"
      ]
    }
  }
}
