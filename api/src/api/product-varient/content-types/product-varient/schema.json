        "videos",
        "audios"
      ]
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_varients"
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "product_varients"
    }
  }
